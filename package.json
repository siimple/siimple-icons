    "dependencies": {},
    "devDependencies": {
        "get-args": "^2.0.0",
        "clean-css-cli": "^4.2.1",
        "postcss-cli": "^6.0.0",
        "autoprefixer": "^9.1.0",
        "svgicons2svgfont": "^9.0.2",
        "stattic": "^0.3.2",
        "handlebars": "^4.0.11",
        "glob": "^7.1.2",
        "sass": "^1.8.0",
        "sass-lint": "^1.12.1",
        "svgo": "^1.0.5",
        "svgstore": "^2.0.3",
        "svg2ttf": "^4.1.0",
        "ttf2woff": "^2.0.1",
        "wawoff2": "^1.0.2"
    },
    "scripts": {
        "build": "node ./scripts/build.js",
        "docs": "npm run docs:build && npm run docs:serve",
        "docs:build": "node ./scripts/docs.js",
        "docs:deploy": "",
        "docs:serve": "node ./node_modules/stattic/cli.js --folder ./docs/dist --port 5000 --cors",
        "templates": "node ./scripts/templates.js",
        "templates:scss": "node ./scripts/templates.js --flow-tasks generate:scss",
        "templates:test": "node ./scripts/templates.js --flow-tasks generate:test",
        "test": "node ./node_modules/stattic/cli.js --folder ./ --port 5000 --cors"
    }
