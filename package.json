{
    "name": "siimple-icons",
    "version": "0.0.0",
    "description": "A suite of scalable icons for web, desktop and mobile projects",
    "author": "Josemi Juanes <josemijuanes@gmail.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/siimple/siimple-icons"
    },
    "dependencies": {},
    "devDependencies": {
        "rmr": "^0.1.0",
        "svgicons2svgfont": "^9.0.2",
        "stattic": "^0.3.2",
        "kofi": "^0.1.2",
        "keue": "^0.3.0",
        "time-stamp": "^2.0.0",
        "handlebars": "^4.0.11",
        "glob": "^7.1.2",
        "mkdirp": "^0.5.1",
        "node-sass": "^4.7.2",
        "svgo": "^1.0.5",
        "svgstore": "^2.0.3",
        "svg2ttf": "^4.1.0",
        "ttf2woff": "^2.0.1",
        "ttf2woff2": "^2.0.3",
        "tinyflow": "^0.0.3"
    },
    "scripts": {
        "dev": "node ./node_modules/stattic/cli.js --port 5000 --cors",
        "build": "node ./scripts/build.js",
        "docs": "npm run docs:build && npm run docs:serve",
        "docs:build": "node ./scripts/docs.js",
        "docs:deploy": "",
        "docs:serve": "node ./node_modules/stattic/cli.js --folder ./docs/dist --port 5000 --cors",
        "templates": "node ./scripts/templates.js",
        "templates:scss": "node ./scripts/templates.js --flow-tasks generate:scss",
        "templates:test": "node ./scripts/templates.js --flow-tasks generate:test",
        "test": "node ./node_modules/stattic/cli.js --folder ./ --port 5000 --cors"
    }
}
